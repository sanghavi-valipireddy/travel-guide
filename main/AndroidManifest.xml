<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ibtspl.foodmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ibtspl.foodmanagement.MainActivity" />
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ibtspl.foodmanagement.MainActivity" />
        </activity>
        <activity
            android:name=".MenuActivity"
            android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ibtspl.foodmanagement.LoginActivity" />
        </activity>
        <activity android:name=".ThankActivity" />
        <activity
            android:name=".Count"
            android:parentActivityName=".MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ibtspl.foodmanagement.MenuActivity" />
        </activity>
        <activity
            android:name=".CountdisplayActivity"
            android:parentActivityName=".Count">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ibtspl.foodmanagement.Count" />
        </activity>
        <activity android:name=".Ad_Menu" />
        <activity android:name=".Ad_Menu2" />
        <activity android:name=".Ad_menu3" />
        <activity
            android:name=".Admin"
            android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ibtspl.foodmanagement.LoginActivity" />
        </activity>
        <activity android:name=".DateActivity" />
        <activity android:name=".HelpActivity"></activity>
    </application>

